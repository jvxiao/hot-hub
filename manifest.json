{
  "manifest_version": 3,
  "name": "Hot hub",
  "version": "1.0",
  "description": "A Chrome extension collects different hot news ",
  "icons": {
    "16": "icons/default1.png",
    "48": "icons/default1.png",
    "128": "icons/default1.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/default1.png"
  },
  "permissions": ["storage"],
  "background": {
    "service_worker": "background.js"
  }
}